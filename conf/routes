# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET		/revisions/analyse/:article				controllers.WikiController.analyze(article : String, timescope:  String)
GET		/revisions/guesstime/:article				controllers.WikiController.guessAnalyzeTime(article : String, timescope:  String)

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
